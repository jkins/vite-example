{
  "name": "vite-sample",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/main.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --reporter=verbose",
    "test:dev": "vitest watch",
    "test:ui": "vitest --ui",
    "lint": "eslint --ext ts,test.ts,js,vue --ignore-path .gitignore .",
    "docs:build-all": "run-p docs:tests docs:coverage docs:api docs:components && vitepress build docs",
    "docs:api": "typedoc",
    "docs:components": "vue-docgen",
    "docs:coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:tests": "vitest run --reporter=html --outputFile=./docs/public/tests/index.html"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.29.25",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.4",
    "vue": "^3.2.47",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@originjs/vite-plugin-federation": "^1.2.3",
    "@types/lodash": "^4.14.195",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-v8": "^0.33",
    "@vitest/ui": "^0.33",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "chalk": "^5.3.0",
    "eslint": "^8.43.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-jsdoc": "^46.4.0",
    "eslint-plugin-vitest": "^0.2.6",
    "eslint-plugin-vue": "^9.15.1",
    "glob": "^10.3.1",
    "happy-dom": "^10.0.3",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.63.6",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-static-copy": "^0.17.0",
    "vitepress": "^1.0.0-beta.5",
    "vitepress-plugin-sidebar": "^0.0.7",
    "vitest": "^0.34",
    "vue-docgen-cli": "^4.67.0",
    "vue-tsc": "^1.4.2"
  },
  "msw": {
    "workerDirectory": "docs/public"
  }
}
